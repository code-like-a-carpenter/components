{
  "name": "@code-like-a-carpenter/components",
  "version": "0.0.0-development",
  "description": "A collection of components I can use accross projects",
  "bin": {},
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-like-a-carpenter/components.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:cjs": "rimraf dist/cjs",
    "build:cjs": "BUILD_TARGET=cjs babel --source-maps --extensions '.js,.ts,.tsx' -d dist/cjs src",
    "postbuild:cjs": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "prebuild:esm": "rimraf dist/esm",
    "build:esm": "BUILD_TARGET=esm babel --source-maps --extensions '.js,.ts,.tsx' -d dist/esm src",
    "postbuild:esm": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:types": "tsc --emitDeclarationOnly ",
    "build-storybook": "build-storybook",
    "eslint": "eslint ${ESLINT_FORMAT_OPTIONS:-} --ignore --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:es": "npm run --silent eslint -- .",
    "prelint:types": "mkdirp reports/style",
    "lint:types": "bash -c \"tsc --noEmit $TSC_OPTIONS\" ",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "test": "TZ=UTC jest"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-like-a-carpenter/components/issues"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "dedent": "^0.7.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ianwremmel/eslint-plugin-ianwremmel": "^4.1.1",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/addon-storyshots": "^6.0.22",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.22",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.2.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "lint-staged": "^10.4.0",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "semantic-release": "^17.1.2",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.3"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
