{
  "name": "@code-like-a-carpenter/components",
  "version": "0.0.0-development",
  "description": "A collection of components I can use accross projects",
  "bin": {},
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-like-a-carpenter/components.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:cjs": "rimraf dist/cjs",
    "build:cjs": "BUILD_TARGET=cjs babel --source-maps --extensions '.js,.ts,.tsx' -d dist/cjs src",
    "postbuild:cjs": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "prebuild:esm": "rimraf dist/esm",
    "build:esm": "BUILD_TARGET=esm babel --source-maps --extensions '.js,.ts,.tsx' -d dist/esm src",
    "postbuild:esm": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:types": "tsc --emitDeclarationOnly ",
    "build-storybook": "build-storybook",
    "eslint": "eslint ${ESLINT_FORMAT_OPTIONS:-} --ignore --ignore-path .gitignore",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:es": "npm run --silent eslint -- .",
    "prelint:types": "mkdirp reports/style",
    "lint:types": "bash -c \"tsc --noEmit $TSC_OPTIONS\" ",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "test": "TZ=UTC jest"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-like-a-carpenter/components/issues"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "dedent": "^0.7.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@faker-js/faker": "^7.6.0",
    "@ianwremmel/eslint-plugin-ianwremmel": "^6.2.0",
    "@storybook/addon-actions": "~6.5.9",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/addon-links": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5",
    "@storybook/manager-webpack5": "~6.5",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/react": "~6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^28.1.8",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.7.2",
    "css-loader": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^14.0.1",
    "lint-staged": "^13.2.2",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "semantic-release": "^19.0.5",
    "style-loader": "^2.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
  },
  "publishConfig": {
    "access": "public"
  }
}
