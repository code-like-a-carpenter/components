{
  "name": "@code-like-a-carpenter/components",
  "version": "0.0.0-development",
  "description": "A collection of components I can use accross projects",
  "bin": {},
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/types/index.d.ts",
  "sass": "src/style.scss",
  "style": "dist/style.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-like-a-carpenter/components.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:cjs": "rimraf dist/cjs",
    "find-files-to-build": "find ./src -name '*.stories.tsx' -prune -o -name '*.spec.*' -prune -o -name '*.tsx' -print -o -name '*.ts' -print",
    "build:cjs": "esbuild $(npm run --silent find-files-to-build) --format=cjs --outdir=dist/cjs --platform=node --sourcemap=external",
    "postbuild:cjs": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "prebuild:esm": "rimraf dist/esm",
    "build:esm": "esbuild $(npm run --silent find-files-to-build) --format=esm --outdir=dist/esm --platform=node --sourcemap=external",
    "postbuild:esm": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:styles": "sass src/style.scss dist/style.css",
    "build:types": "tsc --emitDeclarationOnly ",
    "build-storybook": "storybook build",
    "eslint": "eslint ${ESLINT_FORMAT_OPTIONS:-} --ignore --ignore-path .gitignore",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:es": "npm run --silent eslint -- .",
    "prelint:types": "mkdirp reports/style",
    "lint:types": "bash -c \"tsc --noEmit $TSC_OPTIONS\" ",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "storybook": "storybook dev -p 6006",
    "test": "TZ=UTC jest"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-like-a-carpenter/components/issues"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "classnames": "^2.3.2",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "dedent": "^0.7.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "recharts": "^2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^17.6.3",
    "@faker-js/faker": "^8.0.2",
    "@ianwremmel/eslint-plugin-ianwremmel": "^6.2.0",
    "@storybook/addon-actions": "~7.0.18",
    "@storybook/addon-essentials": "~7.0.18",
    "@storybook/addon-links": "~7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "~7.0.18",
    "@storybook/react-webpack5": "~7.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/d3-array": "^3.0.5",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29.5.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/sass": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@visx/mock-data": "^3.0.0",
    "chromatic": "^6.17.4",
    "esbuild": "^0.18.9",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.2.2",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "semantic-release": "^21.0.2",
    "storybook": "~7.0.18",
    "typescript": "^5.0.4",
    "url": "^0.11.0"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
  },
  "publishConfig": {
    "access": "public"
  }
}
