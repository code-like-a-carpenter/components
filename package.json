{
  "name": "@code-like-a-carpenter/components",
  "version": "0.0.0-development",
  "description": "A collection of components I can use accross projects",
  "bin": {},
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-like-a-carpenter/components.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:cjs": "rimraf dist/cjs",
    "build:cjs": "BUILD_TARGET=cjs babel --source-maps --extensions '.js,.ts,.tsx' -d dist/cjs src",
    "postbuild:cjs": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "prebuild:esm": "rimraf dist/esm",
    "build:esm": "BUILD_TARGET=esm babel --source-maps --extensions '.js,.ts,.tsx' -d dist/esm src",
    "postbuild:esm": "rimraf 'dist/**/*.stories.*' 'dist/**/*.spec.*'",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:types": "tsc --emitDeclarationOnly ",
    "build-storybook": "build-storybook",
    "eslint": "eslint ${ESLINT_FORMAT_OPTIONS:-} --ignore --ignore-path .gitignore",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:es": "npm run --silent eslint -- .",
    "prelint:types": "mkdirp reports/style",
    "lint:types": "bash -c \"tsc --noEmit $TSC_OPTIONS\" ",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "test": "TZ=UTC jest"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-like-a-carpenter/components/issues"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "classnames": "^2.2.6",
    "dedent": "^0.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.31",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-markdown": "^4.3.1",
    "react-moment": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@ianwremmel/eslint-plugin-ianwremmel": "^4.3.1",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-storyshots": "^6.3.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@types/dedent": "^0.7.0",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.2.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.4.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.0",
    "jest-junit": "^12.0.0",
    "lint-staged": "^11.0.0",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "semantic-release": "^17.1.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
